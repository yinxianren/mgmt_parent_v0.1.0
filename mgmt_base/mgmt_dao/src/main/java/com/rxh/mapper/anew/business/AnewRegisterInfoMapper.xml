<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.rxh.mapper.anew.business.AnewRegisterInfoMapper" >

    <insert id="replaceSave"  parameterType="com.rxh.anew.table.business.RegisterInfoTable" >
        replace into b1_merchant_into_info_main value (
        #{id},
        #{merchantId},
        #{terminalMerId},
        #{terminalMerName},
        #{userName},
        #{userShortName},
        #{identityType},
        #{identityNum},
        #{phone},
        #{merchantType},
        #{province},
        #{city},
        #{address},
        #{status},
        #{createTime},
        #{updateTime}
        );
    </insert>
</mapper>
